{
  "compilerOptions": {
    "outDir": "../aot",
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "declaration": true,
    "skipLibCheck": true,
    "stripInternal": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "lib": [
      "es2015",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "../node_modules",
    "../node_modules/**"
  ],
  "files": [
    "app.module.ts",
    "typings.d.ts"
  ],
  "angularCompilerOptions": {
    "skipMetadataEmit": false,
    "skipTemplateCodegen": true
  }
}