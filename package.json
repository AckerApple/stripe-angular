{
  "name": "stripe-angular",
  "version": "0.0.0",
  "description": "Angular to Stripe module containing useful providers, components, and directives",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "update:dist": "node scripts/update-dist-package.js",
    "ack-webpack": "ack-webpack",
    "build:dist": "ngc --declaration --project src",
    "test:watch": "ng test",
    "build:index": "pug app/src/index.pug --out app/www",
    "compile:templates:app": "ack-pug-bundler app/src/components/pugs app/src/components/templates --outType ts --oneToOne",
    "compile:templates:src": "ack-pug-bundler src/components/pugs src/components/templates --outType ts --oneToOne",
    "compile:templates": "npm-run-all compile:templates:app compile:templates:src",
    "watch:templates": "npm-run-all --parallel watch:templates:app watch:templates:src watch:app-template",
    "watch:templates:app": "npm run compile:templates:app -- --watch",
    "watch:templates:src": "npm run compile:templates:src -- --watch",
    "watch:app-template": "npm run compile:app-template -- --watch",
    "build:js": "node --max-old-space-size=8192 ./node_modules/ack-webpack/bin/cli app/src/index.prod.ts app/www/assets/scripts/index.js --production --project app/src/tsconfig.json",
    "watch:js": "ack-webpack app/src/index.ts app/www/assets/scripts/index.js --project app/src/tsconfig.json --watch --browser=app/www",
    "build:css": "ack-sass app/src/styles.scss app/www/assets/styles/styles.css --production",
    "watch:css": "ack-sass app/src/styles.scss app/www/assets/styles/styles.css --watch",
    "watch": "npm-run-all --parallel compile:templates watch:css watch:templates watch:js",
    "build": "npm-run-all -s build:dist build:css build:index compile:templates build:js update:dist",
    "compile:app-template": "ack-pug-bundler app/src/components/app.component.ts app/src/components/templates/ --skipRender --oneFile --outType ts --outFileExt .template.ts",
    "test": "ng test --browser PhantomJS --single-run",
    "start": "ack-reload -d app/www --port 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckerApple/stripe-angular.git"
  },
  "keywords": [
    "stripe",
    "angular",
    "credit",
    "debit",
    "card",
    "payment",
    "gateway"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AckerApple/stripe-angular/issues"
  },
  "homepage": "https://github.com/AckerApple/stripe-angular#readme",
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/compiler-cli": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@types/jasmine": "^2.8.3",
    "ack-pug-bundler": "^1.3.14",
    "ack-sass": "^1.1.1",
    "ack-webpack": "^1.2.2",
    "classlist-polyfill": "^1.2.0",
    "core-js": "^2.5.3",
    "jasmine": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "npm-run-all": "^4.1.2",
    "phantomjs-prebuilt": "^2.1.16",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6",
    "pug-loader": "^2.3.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.6",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "zone.js": "^0.8.19"
  },
  "scriptsInfo": {
    "build": "Builds TypeScript files to output folder",
    "test:watch": "Opens local browser to test angular app",
    "build:index": "Casts index.pug layout into index.html template to act as app entry file",
    "compile:templates": "Casts pugs folder, that has layout files like template.pug, into TypeScript compatible template.pug.ts files",
    "watch:templates": "Watches pugs folder for changes to engage the script compile:templates",
    "build:js": "Builds one TypeScript source file into a final www output file. Node memory has been increased to cover large app builds",
    "watch:js": "Builds one TypeScript source file into a final www output file. Node memory has been increased to cover large app builds",
    "build:css": "Builds one SASS source file into a final css output file",
    "watch:css": "Builds one SASS source file into a final css output file",
    "watch": "Employes other watch script to particpate in a combined",
    "test": "Tests angular app using karama and jasmine",
    "start": "Opens browser pointed at www folder"
  },
  "private": true
}
