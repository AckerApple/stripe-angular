{
  "name": "stripe-angular-gh-pages",
  "version": "0.0.8",
  "main": "server",
  "description": "Angular to Stripe module containing useful providers, components, and directives",
  "scripts": {
    "start": "npm run start:server",
    "start:app": "ng serve --open",
    "start:server": "node .",
    "watch": "npm run start:server & npm run start:app",
    "build": "ng build src --stats-json --prod --output-hashing=none",
    "save": "git add . && git commit -m 'update' && git push",
    "deploy": "npm run patch:version && npm run build && npm run copy:www && npm run save",
    "copy:www": "cp -R www/. .",
    "patch:version": "npm --no-git-tag-version version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckerApple/stripe-angular.git"
  },
  "keywords": [
    "stripe",
    "angular",
    "credit",
    "debit",
    "card",
    "payment",
    "gateway"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AckerApple/stripe-angular/issues"
  },
  "homepage": "https://github.com/AckerApple/stripe-angular#readme",
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/common": "^11.0.7",
    "@angular/compiler": "^11.0.7",
    "@angular/compiler-cli": "^11.0.7",
    "@angular/core": "^11.0.7",
    "@angular/platform-browser": "^11.0.7",
    "@angular/platform-browser-dynamic": "^11.0.7",
    "@types/stripe-v3": "^3.1.23",
    "ack-css-boot": "^2.2.2",
    "rxjs": "^6.6.3",
    "stripe-angular": "git+https://github.com/ackerapple/stripe-angular.git",
    "typescript": "4.0.5",
    "zone.js": "^0.11.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "crypto-js": "^4.0.0",
    "express": "^4.17.1",
    "form-urlencoded": "^4.2.1"
  }
}
